type FormField{
    id: ID!
    name: String!
    fieldType: String
    defaultFieldValue: String
    fieldValue: String
    isRequired: Boolean
}

type Task {
  id: ID!
  name: String!
  formFields: [FormField]
  className: String
  classBeanNameReference: String
}

extend type Query {
    findAllTasks: [Task]!
    countTasks: Long!
}

extend type Mutation {
    newTask(name: String!, formFields: [FormField], className: String, classBeanNameReference: String) : Task!
}
